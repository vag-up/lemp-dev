Listen 8080
<VirtualHost _default_:8080>
    DocumentRoot /usr/share/phpmyadmin
    <Directory /usr/share/phpmyadmin>
        AllowOverride All
    </Directory>
    <FilesMatch \.php$>
		SetHandler "proxy:unix:/var/run/php/php{{ php_version }}-fpm.sock|fcgi://localhost"
	</FilesMatch>
</VirtualHost>
